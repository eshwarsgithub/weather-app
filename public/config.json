{
    "workflowApiVersion": "1.1",
    "key": "weather-check-activity-key",
    "name": "Weather Check Activity",
    "description": "Routes contacts based on current weather conditions at their location",
    "type": "RESTDECISION",
    "lang": {
      "en-US": {
        "name": "Weather Check Activity",
        "description": "Routes contacts based on weather conditions"
      }
    },
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "contactKey": "{{Contact.Key}}",
            "emailAddress": "{{Contact.Attribute.EmailAddress}}",
            "city": "{{Event.JourneyEntry_WeatherCheck_DE.City}}",
            "state": "{{Event.JourneyEntry_WeatherCheck_DE.State}}",
            "postalCode": "{{Event.JourneyEntry_WeatherCheck_DE.PostalCode}}",
            "country": "{{Event.JourneyEntry_WeatherCheck_DE.Country}}",
            "latitude": "{{Event.JourneyEntry_WeatherCheck_DE.Latitude}}",
            "longitude": "{{Event.JourneyEntry_WeatherCheck_DE.Longitude}}"
          }
        ],
        "outArguments": [
          {
            "weatherCondition": "",
            "temperature": "",
            "humidity": "",
            "description": "",
            "branchResult": ""
          }
        ],
        "url": "https://weather-app-kappa-three-25.vercel.app/execute",
        "verb": "POST",
        "useJwt": false,
        "timeout": 10000
      }
    },
    "configurationArguments": {
      "save": { "url": "https://weather-app-kappa-three-25.vercel.app/save" },
      "publish": { "url": "https://weather-app-kappa-three-25.vercel.app/publish" },
      "validate": { "url": "https://weather-app-kappa-three-25.vercel.app/validate" },
      "stop": { "url": "https://weather-app-kappa-three-25.vercel.app/stop" }
    },
    "edit": {
      "url": "https://weather-app-kappa-three-25.vercel.app/edit.html"
    },
    "outcomes": [
      {
        "key": "adverse_weather",
        "name": "Adverse Weather",
        "arguments": {
          "branchResult": "Adverse Weather"
        }
      },
      {
        "key": "good_weather",
        "name": "Good Weather",
        "arguments": {
          "branchResult": "Good Weather"
        }
      }
    ],
    "metaData": {
      "icon": "images/icon.png",
      "category": "Decision"
    },
    "isConfigured": true
  }
  
  