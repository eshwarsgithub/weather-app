{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/config.json",
      "dest": "/public/config.json"
    },
    {
      "src": "/edit.html",
      "dest": "/public/edit.html"
    },
    {
      "src": "/customActivity.js",
      "dest": "/public/customActivity.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|json|html))$",
      "dest": "/public/$1"
    }
  ]
}